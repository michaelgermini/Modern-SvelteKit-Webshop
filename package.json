{
  "name": "webshop-sveltekit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "vite dev",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:suite": "node scripts/test-suite.js",
    "test:products": "node scripts/test-products.js",
    "test:product-pages": "node scripts/test-product-pages.js",
    "generate:images": "node scripts/generate-product-images.js",
    "convert:images": "node scripts/convert-svg-to-png.js",
    "optimize:images": "node scripts/convert-svg-to-png.js --optimize",
    "test:performance": "node scripts/test-suite.js --performance",
    "test:accessibility": "node scripts/test-suite.js --accessibility"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.32",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.3.6",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "stripe": "^14.10.0",
    "zod": "^3.22.4"
  },
  "type": "module"
}
